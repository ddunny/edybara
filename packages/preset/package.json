{
  "name": "@edybara-editor/preset",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "tsup --watch --tsconfig tsconfig.build.json",
    "clean:dist": "rimraf dist",
    "clean:dependency": "rimraf node_modules",
    "build": "npm run clean:dist && npm run build:ts && npm run build:style",
    "build:ts": "tsup --tsconfig tsconfig.build.json",
    "build:style": "sass styles:styles"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "style": "styles/preset.css",
  "types": "dist/index.d.ts",
  "private": true,
  "dependencies": {
    "@edybara-editor/blockquote": "*",
    "@edybara-editor/bold": "*",
    "@edybara-editor/code": "*",
    "@edybara-editor/codeblock": "*",
    "@edybara-editor/command": "*",
    "@edybara-editor/core": "*",
    "@edybara-editor/emoji": "*",
    "@edybara-editor/flat-list": "*",
    "@edybara-editor/flat-task-list": "*",
    "@edybara-editor/font-family": "*",
    "@edybara-editor/heading": "*",
    "@edybara-editor/hr": "*",
    "@edybara-editor/image": "*",
    "@edybara-editor/italic": "*",
    "@edybara-editor/link": "*",
    "@edybara-editor/mention": "*",
    "@edybara-editor/menubar": "*",
    "@edybara-editor/paragraph": "*",
    "@edybara-editor/strikethrough": "*",
    "@edybara-editor/subscript": "*",
    "@edybara-editor/superscript": "*",
    "@edybara-editor/tables": "*",
    "@edybara-editor/text-color": "*",
    "@edybara-editor/ui": "*",
    "@edybara-editor/underline": "*",
    "prosemirror-commands": "*",
    "prosemirror-inputrules": "*",
    "prosemirror-keymap": "*",
    "prosemirror-model": "*",
    "prosemirror-state": "*"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2"
  }
}
