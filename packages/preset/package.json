{
  "name": "@edybara/preset",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "tsup --watch --tsconfig tsconfig.build.json",
    "clean:dist": "rimraf dist",
    "clean:dependency": "rimraf node_modules",
    "build": "npm run clean:dist && npm run build:ts && npm run build:style",
    "build:ts": "tsup --tsconfig tsconfig.build.json",
    "build:style": "sass styles:styles"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "style": "styles/preset.css",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@edybara/blockquote": "*",
    "@edybara/bold": "*",
    "@edybara/code": "*",
    "@edybara/codeblock": "*",
    "@edybara/command": "*",
    "@edybara/core": "*",
    "@edybara/emoji": "*",
    "@edybara/flat-list": "*",
    "@edybara/flat-task-list": "*",
    "@edybara/font-family": "*",
    "@edybara/heading": "*",
    "@edybara/hr": "*",
    "@edybara/image": "*",
    "@edybara/italic": "*",
    "@edybara/link": "*",
    "@edybara/menubar": "*",
    "@edybara/paragraph": "*",
    "@edybara/strikethrough": "*",
    "@edybara/subscript": "*",
    "@edybara/superscript": "*",
    "@edybara/tables": "*",
    "@edybara/text-color": "*",
    "@edybara/ui": "*",
    "@edybara/underline": "*",
    "prosemirror-commands": "*",
    "prosemirror-inputrules": "*",
    "prosemirror-keymap": "*",
    "prosemirror-model": "*",
    "prosemirror-state": "*"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2"
  }
}
